<launch>
  <arg name="robot_id" value="$(optenv ROBOT_ID unnamed_robot)"/>

  <group ns="$(arg robot_id)">
    <node pkg="robot_omni4_gazebo_driver" type="robot_omni4_gazebo_driver_node"
        name="robot_omni4_gazebo_driver" clear_params="true"
        output="$(optenv ROBOT_OUTPUT screen)">
      <rosparam command="load" subst_value="True"
          file="$(find robot_omni4_gazebo_driver)/config/robot_omni4_gazebo_driver.yaml"/>
    </node>
  </group>
</launch>
